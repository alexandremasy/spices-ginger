{
  "author": "Alexandre Masy <hello@alexandremasy.com",
  "description": "A minimalist UMD module loader and handler. Allows you to create distributed web application.",
  "dependencies": {
    "@spices/cayenne": "^0.0.3"
  },
  "devDependencies": {
    "rollup": "^2.21.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "chokidar": "^3.3.1",
    "node-notifier": "^7.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "files": [
    "src/*.js"
  ],
  "keywords": [
    "vue",
    "umd",
    "umd handler"
  ],
  "license": "UNLICENSED",
  "main": "src/index.js",
  "name": "@spices/ginger",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.infinity-commerce.io/"
  },
  "scripts": {
    "analyse": "webpack --json > stats.json",
    "bundle": "webpack --mode=production",
    "start": "nodemon --exec \"yarn bundle\"",
    "prepublish": "yarn bundle"
  },
  "version": "0.6.6",
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "js"
  }
}
